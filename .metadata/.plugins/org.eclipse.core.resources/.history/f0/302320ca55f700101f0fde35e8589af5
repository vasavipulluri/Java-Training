package com.ibm.arrays;

public class Application
{
	public static void main( String[] args )
	{
		Integer numbers[];
		
		numbers =new Integer[5];
		
		System.out.println(numbers.length);
		
		numbers[0]=1;
		numbers[1]=2;
		numbers[2]=3;
		numbers[3]=4;
		numbers[4]=5;
		
		for(int num:numbers) {
			System.out.println(num);
		}
	}
}